{
  "name": "tactile-interface-react-electron",
  "author": "Nigel Gilbert",
  "version": "1.0.4",
  "description": "Desktop application with react, electron and three.js.",
  "main": "main.js",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },

  "eslintConfig": {
    "env": {
      "es6": true
    }
  },
  "scripts": {
    "lint": "eslint .",
    "start": "electron .",
    "start-dev": "NODE_ENV=development electron .",
    "clear": "rm app/js/bundle.js",
    "bundle": "webpack -p",
    "watch": "webpack-dev-server --content-base src/ --inline --hot"
  },
  "dependencies": {
    "async": "^1.4.2",
    "chokidar": "^1.4.3",
    "electron-debug": "^0.2.1",
    "electron-prebuilt": "^0.34.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "require-dir": "^0.3.0",
    "three": "^0.72.0",
    "tween.js": "^16.3.4"
  },
  "devDependencies": {
    "eslint": "^2.0.0",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.3",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.12.1",
    "electron-prebuilt": "^0.34.0"
  }
}